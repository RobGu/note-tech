{
  "[go]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.tabSize": 4
  },
  "cSpell.userWords": [
    "Upsert",
    "Weixin",
    "augmentum",
    "bodyparser",
    "stylelint",
    "weapp",
    "weappsdk",
    "wechat",
    "wxapp"
  ],
  "css.validate": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.fontSize": 20,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  "eslint.autoFixOnSave": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.wxml": "html",
    "*.wxs": "javascript",
    "*.wxss": "css",
    "*rc": "json"
  },
  "files.autoSave": "onWindowChange",
  "files.autoSaveDelay": 60000,
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreLimitWarning": true,
  "gitlens.advanced.messages": {
    "suppressCommitNotFoundWarning": true,
    "suppressLineUncommittedWarning": true,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": true
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.remotes": [
    {
      "domain": "http://gitee.com/",
      "name": "gitee",
      "type": "Custom",
      "urls": {
        "branch": "https://http://gitee.com//${repo}/commits/${branch}",
        "branches": "https://http://gitee.com//${repo}/branches",
        "commit": "https://http://gitee.com//${repo}/commit/${id}",
        "file": "https://http://gitee.com//${repo}?path=${file}${line}",
        "fileInBranch": "https://http://gitee.com//${repo}/blob/${branch}/${file}${line}",
        "fileInCommit": "https://http://gitee.com//${repo}/blob/${id}/${file}${line}",
        "fileLine": "#L${line}",
        "fileRange": "#L${start}-L${end}",
        "repository": "https://http://gitee.com//${repo}"
      }
    }
  ],
  "javascript.validate.enable": false,
  "json.schemas": [
    {
      "fileMatch": [
        "*.swagger.json"
      ],
      "url": "http://json.schemastore.org/swagger-2.0"
    }
  ],
  "mdmath.delimiters": "gitlab",
  "pasteImage.path": "${currentFileDir}/images",
  "prettier.eslintIntegration": true,
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    ".vscode": true
  },
  "search.location": "sidebar",
  "sort-imports.suppress-warnings": true,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.shell.linux": "/bin/zsh",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "typescript.check.npmIsInstalled": false,
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true
}
